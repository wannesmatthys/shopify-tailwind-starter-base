        "title",
        "caption",
        "price",
        "variant_picker",
        "quantity_selector",
        "buy_buttons",
        "description",
        "collapsible-row-0",
        "collapsible-row-1",
        "61e57daf-de8c-4a82-a668-6bb41de0f560",
        "share"
